@model MujZavod.Admin.Models.Race.RaceCategory.RaceCategoryViewModel


@using (Html.BeginForm("CategoryEdit", "Race", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{

    @Html.ValidationSummary("", new { @class = "text-danger" })

    @Html.HiddenFor(x => x.Id)
    @Html.HiddenFor(x => x.RaceId)

    @Html.MzEditLineFor(x => x.Name)

    @Html.MzEditLineFor(x => x.Start)
 
    

    @Html.LabelFor(x => x.Description)
    @Html.MzTextEditorFor(x => x.Description)
}
